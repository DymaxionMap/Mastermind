{
  "extends": "airbnb",
  "parserOptions": {
      "ecmaVersion": 8
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "object-curly-newline": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "react/prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}